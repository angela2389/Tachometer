<h1>Team</h1><br>
<h4><%= @team.manager.role %><h4>
<h2><%="#{@team.manager.user.first_name} #{@team.manager.user.last_name} "%></h2>
<h4><%=@team.manager.user.email%></h4>-------<br><br>
<% @team.team_members[1..-2].each do |member| %>
<h4><%= member.role %><h4>
<h2><%="#{member.user.first_name} #{member.user.last_name} "%></h2>
<h4><%= member.user.email%></h4><p><%= link_to 'Destroy', project_team_member_path(member.project, member), method: :delete, data: { confirm: 'Are you sure?' } %></p>-------<br><br>

<% end %>
<p>Add team member</p>
<%= render 'team_members/form' %>
